{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start -P default --only functions",
    "shell": "firebase functions:shell -P default",
    "start": "npm run shell",
    "deploy-staging": "firebase deploy -P default --only functions",
    "deploy-prod": "firebase deploy -P prod --only functions",
    "logs": "firebase functions:log -P default"
  },
  "engines": {
    "node": "14"
  },
  "main": "index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "ejs": "^3.1.6",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.15.7",
    "googleapis": "^68.0.0",
    "html-pdf-node": "^1.0.8",
    "html-to-text": "^7.0.0",
    "moment": "^2.29.1",
    "nodemailer": "^6.5.0",
    "randomstring": "^1.2.1",
    "stopword": "^1.0.7",
    "stripchar": "^1.2.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
